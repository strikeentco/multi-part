    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "got": "^12.5.3",
    "mocha": "^6.2.3",
    "multer": "^1.4.4",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "vinyl": "^2.2.1"
